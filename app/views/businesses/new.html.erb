<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">New Business</h1>
  </div>
</div>
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        Business Information
      </div>
      <div class="panel-body">
        <div class="row">
          <%= form_for @business, url: businesses_path, method: :post, html: { class: "form-horizontal business" } do |f| %>
            <%= render partial: 'form', locals: { f: f } %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
<%= content_for(:footer) do %>
  <script src='<%= "https://maps.googleapis.com/maps/api/js?key=#{ENV["google_api_key"]}&libraries=places&callback=initAutocomplete" %>'>
  </script>
<% end %>
